<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field" default-cascade="all">
    <class name="org.infinite.identityaccess.domain.model.access.Role" table="tbl_role" lazy="true">
        <id name="id" type="long" column="id" unsaved-value="-1">
            <generator class="native"/>
        </id>
        <version column="concurrency_version" name="concurrencyVersion" type="int" />
        <property name="description" column="description" type="java.lang.String" />
        <many-to-one name="group" class="org.infinite.identityaccess.domain.model.identity.Group" column="group_id" />
        <property name="name" column="name" type="java.lang.String" />
        <property name="supportsNesting" column="supports_nesting" type="boolean" />
        <component name="tenantId" class="org.infinite.identityaccess.domain.model.identity.TenantId">
            <property name="id" column="tenant_id_id" type="java.lang.String" />
        </component>
    </class>
</hibernate-mapping>
